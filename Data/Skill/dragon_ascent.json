"Name": {
"DefaultText": "Dragon Ascent",
"LocalTexts": {
"fr": "Draco Ascension",
"de": "Zenitstürmer",
"es": "Ascenso Draco",
"it": "Ascesa del Drago",
"ko": "화룡점정",
"ja": "ガリョウテンセイ",
